addpath('../maps','../primeP','../complex-potentials')
q = 0.3;
%%%%%%%%%%%%%% Define mappings
% mob = @(zVar) (1-q.^2)./(2i*q).*(zVar+1)./(zVar-1);
% mobD= @(zVar) (1-q.^2)./(2i*q).*(1*(zVar-1)-1*(zVar+1))./(zVar-1).^2;
aoa = pi/8;
% plateA = @(zVar) -exp(-1i*theta).*Pfun(zVar.*exp(2i*theta),q,N)./Pfun(zVar,q,N);
% plateAD= @(zVar) -exp(-1i*theta).*(exp(2i*theta)*PfunD(zVar.*exp(2i*theta),q,N).*Pfun(zVar,q,N) - PfunD(zVar,q,N).*Pfun(zVar.*exp(2i*theta),q,N))./Pfun(zVar,q,N).^2;
% plateD = @(zVar)  -1i*zVar.*PfunD(zVar,q,N)./Pfun(zVar,q,N);
% plateDD = @(zVar) -1i.*PfunD(zVar,q,N)./Pfun(zVar,q,N) - 1i*zVar.*(PfunDD(zVar,q,N).*Pfun(zVar,q,N)-PfunD(zVar,q,N).^2)./Pfun(zVar,q,N).^2;
% gamma = .45*exp(.6i*pi/4);
% circArA = @(zVar) Pfun(zVar.*conj(gamma),q,N)./(Pfun(zVar./gamma,q,N).*Pfun(conj(gamma),q,N)-Pfun(1/gamma,q,N).*Pfun(zVar.*conj(gamma),q,N));
% circArAD = @(zVar) (PfunD(zVar.*conj(gamma),q,N).*conj(gamma).*(Pfun(zVar./gamma,q,N).*Pfun(conj(gamma),q,N)-Pfun(1/gamma,q,N).*Pfun(zVar.*conj(gamma),q,N))- ...
%                     Pfun(zVar.*conj(gamma),q,N).*(PfunD(zVar./gamma,q,N)./gamma.*Pfun(conj(gamma),q,N)-Pfun(1/gamma,q,N).*conj(gamma)*PfunD(zVar.*conj(gamma),q,N)))...
%                    ./(Pfun(zVar./gamma,q,N).*Pfun(conj(gamma),q,N)-Pfun(1/gamma,q,N).*Pfun(zVar.*conj(gamma),q,N)).^2;
% 
% circArB = @(zVar) circArA(zVar)./exp(1i*angle(circArA(-1)-circArA(1i)));
% circArBD = @(zVar) circArAD(zVar)./exp(1i*angle(circArA(-1)-circArA(1i)));
% 
% circAr = @(zVar) circArB(zVar) - circArB(-1);
% circArD = @(zVar) circArBD(zVar);
% 
% phi = 5*pi/8;
% circCe = @(zVar) -Pfun(sqrt(zVar).*exp(1i*phi),q,N).*Pfun(-sqrt(zVar).*exp(1i*phi),q,N).*Pfun(q*sqrt(zVar),q,N).*Pfun(-q*sqrt(zVar),q,N)./...
%                  (Pfun(q*sqrt(zVar).*exp(1i*phi),q,N).*Pfun(-q*sqrt(zVar).*exp(1i*phi),q,N).*Pfun(sqrt(zVar),q,N).*Pfun(-sqrt(zVar),q,N)).*exp(-1i*phi);
% circCe = @(zVar) -Pfun(zVar*exp(2i*phi),q.^2,N).*Pfun(q^2*zVar,q^2,N)./...
%                  (Pfun(q^2*zVar.*exp(2i*phi),q^2,N).*Pfun(zVar,q^2,N)).*exp(-1i*phi);
% circCeD= @(zVar) -((PfunD(zVar*exp(2i*phi),q.^2,N)*exp(2i*phi).*Pfun(q^2*zVar,q^2,N)+Pfun(zVar*exp(2i*phi),q.^2,N).*PfunD(q^2*zVar,q^2,N)*q^2).*...
%                     Pfun(q^2*zVar.*exp(2i*phi),q^2,N).*Pfun(zVar,q^2,N)...
%                   -(q^2*exp(2i*phi)*PfunD(q^2*zVar.*exp(2i*phi),q^2,N).*Pfun(zVar,q^2,N)+Pfun(q^2*zVar.*exp(2i*phi),q^2,N).*PfunD(zVar,q^2,N)).*...
%                   Pfun(zVar*exp(2i*phi),q.^2,N).*Pfun(q^2*zVar,q^2,N))...
%                   ./(Pfun(q^2*zVar.*exp(2i*phi),q^2,N).*Pfun(zVar,q^2,N)).^2.*exp(-1i*phi);        
c0 = exp(1i*linspace(0,2*pi)');
c1 = q*exp(1i*linspace(0,2*pi)');
confFill = [0.9,0.9,0.9];

zg = linspace(q,1,200)'.*exp(1i*linspace(0,2*pi,200));
nga = 10; ngb = 10;
zg(abs(zg-1)<1e-1)=nan;
% circPot = @(zVar) 1/(2i*pi)*log(zVar);
% circVel = @(zVar) 1/(2i*pi)./zVar;
% uniPot = @(zVar) -1i*zVar.*PfunD(zVar,q,N)./Pfun(zVar,q,N);
% uniVel = @(zVar) -1i*(PfunD(zVar,q,N)./Pfun(zVar,q,N)+zVar.*(PfunDD(zVar,q,N).*Pfun(zVar,q,N)-PfunD(zVar,q,N).^2)./Pfun(zVar,q,N).^2);
% vortPos = 1.5*q*exp(1i*(pi/4+.2));
% vortPot = @(zVar) 1/(2i*pi)*log(abs(vortPos)*Pfun(zVar/vortPos,q,N)./Pfun(zVar*conj(vortPos),q,N));
% vortVel = @(zVar) 1/(2i*pi)*(1./vortPos*PfunD(zVar/vortPos,q,N)./Pfun(zVar/vortPos,q,N) - conj(vortPos)*PfunD(zVar*conj(vortPos),q,N)./Pfun(zVar*conj(vortPos),q,N));
% strnPot = @(zVar) (zVar.*PfunD(zVar,q,N)./Pfun(zVar,q,N) + zVar.^2.*PfunDD(zVar,q,N)./Pfun(zVar,q,N) - (zVar.*PfunD(zVar,q,N)./Pfun(zVar,q,N)).^2);

for j = 2;%[1:3,5]
    if j ==1
    % Straining flow with circular wing
    [f,fp,a,d] = circularWing(q);
    vals = 4*[-linspace(0,1,20),linspace(0,1,20)];
    [potential,compVel] = strain(q,a);
    ax = [-4,4,-.2,4];
    elseif j ==2
    % Flat plate wing with circulatory flow
    [f,fp,zt,a,d] = flatWing(aoa,q);
    vals = linspace(0,log(q),20)/2/pi;
    [potential,compVel] = circulatory;
    ax = [-.5,1.3,-.1,1.5];
    elseif j ==3
    % Zero aoa flat plate wing with motion
    f = @(zVar) plateD(zVar) - plateD(-1);
    fp= plateDD;
    vals = linspace(-1,1,50);
    ni = 200;
    zInt = q*exp(1i*linspace(0,2*pi,ni)); zInt4 = permute(zInt,[1,4,3,2]);
    D = f(q); DDot = -1i; aDot = 0;
    I = -1/(2i*pi).*trapz(zInt,Mprime(zInt4,f,D,DDot,aDot)./zInt4,4);
    M = @(zVar) Mprime(zVar,f,D,DDot,aDot)+ I;
    movePot = @(zVar) zVar/pi.*trapz(zInt,M(zInt4).*PfunD(zVar./zInt4,q,N)./Pfun(zVar./zInt4,q,N)./zInt4.^2,4);
    pot  = movePot;
    ax = [-2,2,-.1,2];
    elseif j ==4
    % Circular arc wing with vortex
    f = @(zVar) circAr(zVar);
    fp= @(zVar) circArD(zVar);
    pot = vortPot;
    vel = vortVel;
    vals = linspace(0.01,.5,20);
    ax = .5*[-2,2,-.1,2];
    elseif j ==5
    % Centered circular arc with with uniform flow and Kutta condition   
    thetT = fsolve(@(th) circCeD(q*exp(1i*th)),pi);
    zt = q*exp(1i*thetT);
    circ = -zt*(2i*pi)*uniVel(zt);
    f = @(zVar) circCe(zVar);
    pot = @(zVar) circ*circPot(zVar) + uniPot(zVar);
    vals = imag(pot(q))+linspace(-1,1,30);
    ax = [-3,3,-.1,1.5];
    end

% physical plot
f2 = figure(2);
clf
fill([-5 5 5 -5],[-.2 -.2 0 0],confFill,'EdgeColor',confFill);

hold on

contour(real(f(zg)),imag(f(zg)),imag(potential(zg)),vals,'k','LineWidth',.5)
if j ==5
    col = lines;
    v = [imag(potential(q)) imag(potential(q))];
    contour(real(f(zg)),imag(f(zg)),imag(potential(zg)),v,'LineColor','g','LineWidth',3)
end
if j == 4
   scatter(real(f(vortPos)),imag(f(vortPos)),150,'k','MarkerFaceColor','g') 
end
plot(f(c0),'b','LineWidth',7)
plot(f(c1),'r','LineWidth',7)
if j==1
fill(real(f(c1)),imag(f(c1)),confFill);
end

axis(ax);
hold off
axis off
ax = gca;
set(ax,'Units', 'normalized', 'Position', [0 0 1 1])
rat = range(ax.YLim)./range(ax.XLim);
f2.Position(3) = 1/rat*f2.Position(4);
%print(['z-strm',num2str(j)],'-dpng','-r100');

end

function Mp = Mprime(zVar,f,D,DDot,aDot)

Mp = imag(conj(DDot)*(f(zVar)-D) + 1i*aDot*abs(f(zVar)-D).^2);

end